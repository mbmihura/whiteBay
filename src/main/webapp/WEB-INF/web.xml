<?xml version="1.0" encoding="utf-8"?>
<web-app
        version="2.5"
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
     <servlet>
        <servlet-name>RSS</servlet-name>
        <servlet-class>com.tacs13G6.servlets.RssServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RSS</servlet-name>
        <url-pattern>/rss/*</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Social</servlet-name>
        <servlet-class>com.tacs13G6.servlets.SocialServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Social</servlet-name>
        <url-pattern>/social/*</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Feeds</servlet-name>
        <servlet-class>com.tacs13G6.servlets.FeedServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Feeds</servlet-name>
        <url-pattern>/feeds/*</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>GaeTest</servlet-name>
        <servlet-class>com.tacs13G6.GaeTestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GaeTest</servlet-name>
        <url-pattern>/GaeTest</url-pattern>
    </servlet-mapping>
     <servlet>
        <servlet-name>Hello</servlet-name>
        <servlet-class>com.tacs13G6.HelloServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Hello</servlet-name>
        <url-pattern>/hello/*</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>welcome.jsp</welcome-file>
    </welcome-file-list>
</web-app>
